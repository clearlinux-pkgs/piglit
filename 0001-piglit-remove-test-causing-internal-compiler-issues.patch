From 1ae126a08f75db8b60a9b6ecc9b0c2a5259c6993 Mon Sep 17 00:00:00 2001
From: Daniel Charles <daniel.charles@intel.com>
Date: Mon, 8 Jul 2019 17:28:10 -0700
Subject: [PATCH] piglit: remove test causing internal compiler issues

upstream bug: https://gitlab.freedesktop.org/mesa/piglit/issues/17

Signed-off-by: Daniel Charles <daniel.charles@intel.com>
---
 tests/opengl.py                   | 1 -
 tests/texturing/CMakeLists.gl.txt | 1 -
 2 files changed, 2 deletions(-)

diff --git a/tests/opengl.py b/tests/opengl.py
index b0dc6e584..135a35603 100644
--- a/tests/opengl.py
+++ b/tests/opengl.py
@@ -713,7 +713,6 @@ with profile.test_list.group_manager(
     g(['getteximage-targets', '1D'])
     g(['getteximage-targets', '2D'])
     g(['teximage-scale-bias'])
-    g(['tex-upside-down-miptree'])
 
     for prim in ['GL_POINTS', 'GL_LINE_LOOP', 'GL_LINE_STRIP', 'GL_LINES',
                  'GL_TRIANGLES', 'GL_TRIANGLE_STRIP', 'GL_TRIANGLE_FAN',
diff --git a/tests/texturing/CMakeLists.gl.txt b/tests/texturing/CMakeLists.gl.txt
index 02b572c79..e5d41e432 100644
--- a/tests/texturing/CMakeLists.gl.txt
+++ b/tests/texturing/CMakeLists.gl.txt
@@ -98,6 +98,5 @@ piglit_add_executable (texture-al texture-al.c)
 piglit_add_executable (texture-rg texture-rg.c)
 piglit_add_executable (teximage-colors teximage-colors.c)
 piglit_add_executable (zero-tex-coord zero-tex-coord.c)
-piglit_add_executable (tex-upside-down-miptree tex-upside-down-miptree.c)
 
 # vim: ft=cmake:
